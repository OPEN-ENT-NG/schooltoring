{
    "name": "{{ schooltoringVersion }}",
    "config": {
        "main" : "fr.openent.schooltoring.Schooltoring",
        "port" : 8141,
        "app-name" : "Schooltoring",
        "app-address" : "/schooltoring",
        "app-icon" : "https://{{ host }}/schooltoring/public/img/logo.png",
        "host": "https://{{ host }}",
        "ssl" : true,
        "auto-redeploy": false,
        "userbook-host": "https://{{ host }}",
        "integration-mode" : "HTTP",
        "app-registry.port" : 8012,
        "mode" : "prod",
        "sql": true,
        "entcore.port" : 8009,
        "publicConf": {
            "xiti": {
                "ID_SERVICE": {
                    "default": 2
                },
                "LIBELLE_SERVICE": {
                    "default": "PRODUCTION_COLLABORATIVE"
                }
            }
        },
        "db-schema": "schooltoring",
        "messaging": {
            "uri": "{{ schooltoringUri  }}",
            "tokenUrn": "{{ schooltoringTokenUrn }}",
            "scope": "{{ schooltoringScope }}",
            "url": "{{ schooltoringUrl }}",
            "client_mail": "{{ schooltoringClientMail }}",
            "aud": "{{ schooltoringAud }}",
            "key": "{{ schooltoringKey}}"
        }
    }
}